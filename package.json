{
  "name": "mentiq-sdk",
  "version": "0.1.0",
  "description": "Mentiq JavaScript/TypeScript frontend analytics SDK",
  "keywords": [
    "mentiq",
    "analytics",
    "sdk",
    "frontend",
    "tracking"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.mjs",
      "require": "./dist/react/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "INTEGRATION_GUIDE.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json --emitDeclarationOnly && node scripts/build.mjs",
    "dev": "node scripts/build.mjs",
    "typecheck": "tsc --noEmit",
    "test": "node --version"
  },
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "esbuild": "^0.23.1",
    "typescript": "^5.6.2"
  }
}
